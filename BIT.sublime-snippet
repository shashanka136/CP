/*-----------BIT----------*/
void ins(ll i, ll x){
	while(i<=n){
		bit[i] += x;
		i += (i&-i);
	}
}
ll sum(ll i){
	ll ans = 0;
	while(i > 0){
		ans += bit[i];
		i = (i&i-1);
	}
	return ans;
}
